{
  "name": "nusphere/laminas-sentry-logger",
  "description": "Integrates the sentry logger into your Laminas application",
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Hopfe",
      "email": "nusphere83@gmail.com"
    }
  ],
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "laminas/laminas-modulemanager": "*",
    "laminas/laminas-servicemanager": "*",
    "sentry/sentry-symfony": "*"
  },
  "require-dev": {
    "laminas/laminas-coding-standard": "*"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "NuSphere\\Laminas\\Sentry\\Logger\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NuSphere\\Laminas\\Sentry\\Logger\\Test\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "phpcs": "phpcs -p",
    "phpcbf": "phpcbf -p"
  }
}
